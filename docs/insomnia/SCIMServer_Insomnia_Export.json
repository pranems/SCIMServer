    {
      "_id": "req_create_user",
      "_type": "request",
      "parentId": "",
      "name": "Create User (full)",
      "method": "POST",
      "url": "{{ base_url }}/scim/v2/Users",
      "body": {
        "mimeType": "application/scim+json",
        "text": "{\n  \"schemas\": [\"urn:ietf:params:scim:schemas:core:2.0:User\",\"urn:ietf:params:scim:schemas:extension:enterprise:2.0:User\"],\n  \"userName\": \"alice.example@example.com\",\n  \"externalId\": \"ext-000123\",\n  \"name\": { \"formatted\": \"Alice L Example\", \"familyName\": \"Example\", \"givenName\": \"Alice\" },\n  \"displayName\": \"Alice Example\",\n  \"emails\": [{ \"value\": \"alice.work@example.com\", \"type\": \"work\", \"primary\": true }],\n  \"phoneNumbers\": [{ \"value\": \"+1-425-555-0101\", \"type\": \"work\" }],\n  \"roles\": [{ \"value\": \"engineering:senior\", \"display\": \"Senior Engineer\", \"primary\": true }],\n  \"urn:ietf:params:scim:schemas:extension:enterprise:2.0:User\": { \"employeeNumber\": \"EMP-000123\", \"department\": \"Engineering\", \"manager\": { \"value\": \"mgr-001\", \"displayName\": \"Bob Manager\" } }\n}"
      },
      "headers": [
        { "name": "Content-Type", "value": "application/scim+json" },
        { "name": "Authorization", "value": "Bearer {{ token }}" }
      ]
    },
    {
      "_id": "req_list_users",
      "_type": "request",
      "parentId": "",
      "name": "List Users",
      "method": "GET",
      "url": "{{ base_url }}/scim/v2/Users?startIndex=1&count=50",
      "headers": [ { "name": "Authorization", "value": "Bearer {{ token }}" } ]
    },
    {
      "_id": "req_filter_users",
      "_type": "request",
      "parentId": "",
      "name": "Filter Users (case-insensitive)",
      "method": "GET",
      "url": "{{ base_url }}/scim/v2/Users?filter=userName eq \"ALICE.EXAMPLE@EXAMPLE.COM\"",
      "headers": [ { "name": "Authorization", "value": "Bearer {{ token }}" } ]
    },
    {
      "_id": "req_get_user",
      "_type": "request",
      "parentId": "",
      "name": "Get User",
      "method": "GET",
      "url": "{{ base_url }}/scim/v2/Users/{{ user_id }}",
      "headers": [ { "name": "Authorization", "value": "Bearer {{ token }}" } ]
    },
    {
      "_id": "req_patch_valuepath",
      "_type": "request",
      "parentId": "",
      "name": "Patch User - ValuePath Replace",
      "method": "PATCH",
      "url": "{{ base_url }}/scim/v2/Users/{{ user_id }}",
      "body": {
        "mimeType": "application/scim+json",
        "text": "{\"schemas\":[\"urn:ietf:params:scim:api:messages:2.0:PatchOp\"],\"Operations\":[{\"op\":\"replace\",\"path\":\"emails[type eq \\\"work\\\"].value\",\"value\":\"new@example.com\"},{\"op\":\"replace\",\"path\":\"phoneNumbers[type eq \\\"work\\\"].value\",\"value\":\"+1-425-555-9999\"}]}"
      },
      "headers": [
        { "name": "Content-Type", "value": "application/scim+json" },
        { "name": "Authorization", "value": "Bearer {{ token }}" }
      ]
    },
    {
      "_id": "req_patch_enterprise",
      "_type": "request",
      "parentId": "",
      "name": "Patch User - Enterprise Extension Manager",
      "method": "PATCH",
      "url": "{{ base_url }}/scim/v2/Users/{{ user_id }}",
      "body": {
        "mimeType": "application/scim+json",
        "text": "{\"schemas\":[\"urn:ietf:params:scim:api:messages:2.0:PatchOp\"],\"Operations\":[{\"op\":\"add\",\"path\":\"urn:ietf:params:scim:schemas:extension:enterprise:2.0:User:manager\",\"value\":{\"value\":\"mgr-001\",\"displayName\":\"Bob Manager\"}},{\"op\":\"replace\",\"path\":\"urn:ietf:params:scim:schemas:extension:enterprise:2.0:User:department\",\"value\":\"Platform Engineering\"}]}"
      },
      "headers": [
        { "name": "Content-Type", "value": "application/scim+json" },
        { "name": "Authorization", "value": "Bearer {{ token }}" }
      ]
    },
    {
      "_id": "req_patch_nopath",
