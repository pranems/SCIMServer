      "method": "PATCH",
      "url": "{{ base_url }}/scim/v2/Users/{{ user_id }}",
      "body": {
        "mimeType": "application/scim+json",
        "text": "{\"schemas\":[\"urn:ietf:params:scim:api:messages:2.0:PatchOp\"],\"Operations\":[{\"op\":\"replace\",\"value\":{\"name.givenName\":\"Alicia\",\"name.familyName\":\"Updated\",\"urn:ietf:params:scim:schemas:extension:enterprise:2.0:User:employeeNumber\":\"EMP-999\"}}]}"
      },
      "headers": [
        { "name": "Content-Type", "value": "application/scim+json" },
        { "name": "Authorization", "value": "Bearer {{ token }}" }
      ]
    },
